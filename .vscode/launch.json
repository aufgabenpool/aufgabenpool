{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "cony.py",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/Tools/",
            "program": "${workspaceFolder}/Tools/conv.py",
            "console": "integratedTerminal"
        },
        {
            "name": "get_prewiew_img.js",
            "cwd": "${workspaceFolder}/Tools/",
            "program": "${workspaceFolder}/Tools/get_preview_img.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}